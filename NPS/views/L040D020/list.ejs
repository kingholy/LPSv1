<% include ../_layout/head.ejs %>

<script type="text/javascript">

	function doMoveExamPage(dc_id, dc_name)
	{
		var r = confirm(dc_name+"에 대한 문제를 풀어볼까요?");
		if(r==true)	document.location = "/L040D020/exam/"+dc_id;
		else		return;
	}
	
</script>
		
<form name="userPanel" method='post'> 
			
	<!--  =========================================================================   -->
	
	<table class="default" width="100%" style="vertical-align:top;" border="0" cellspacing="0" cellpadding="0" >
		<colgroup>
			<col width="20px" />
			<col width="200" />
			<col width="" />		
		</colgroup>
		<tr style="height:28px;">
			<td> </td>
			<td class='default tbc_lv1 p_l20' style="text-align:left;border-right:none" >
				<span class="view-title">  
					<SCRIPT language=JavaScript> getMenuName("L040D020"); </SCRIPT> 
				</span>
			</td> 
			<td> </td>
			
		</tr>
		
		
		<!--  =========================================================================   -->
	
		<tr style="height:30px"> 
			<td> </td>
			<td width="600px" colspan="2">
				<span class="questSelLabel"  >
					&nbsp;문제를 풀어볼 단원을 선택하세요 &nbsp; 
			 	</span>
			</td>
		</tr>
		<tr>
			<td> </td>
			<td colspan=2>
				<!--  =========================================================================   -->
			<% 	var sWidth = "600px"; %>
		 	
				<table width="<%=sWidth %>" border="0" cellspacing="0" cellpadding="0" >
					<colgroup>
						<col width="30" /> 
						<col width="100" /> 
						<col width="" /> 
					</colgroup>
					<tr>
						<td class='table-col-header' >NO</td>
						<td class='table-col-header' style="BORDER-LEFT: #B4B4B4 1px solid;"> 이동 </td> 							
						<td class='table-col-header' style="BORDER-LEFT: #B4B4B4 1px solid;"> 단원명 </td> 		
					</tr>
					
				<%
					for (i in vList)	{
				%>			
					<tr class="table-cell">
						<td class='table-cell-left1'  style="text-align:center"  > 
							<%=(parseInt(i)+1)%>
						</td>
						<td class='table-cell2'  style="text-align:center;" > 
							<input value="문제 풀기"  type="button" class="button-item-detail" 
									onclick="doMoveExamPage('<%=vList[i].dc_id%>', '<%=vList[i].dc_category_name %>')"   />
							
						</td>
						<td class='table-cell-right1'  style="text-align:left;padding-left:20px;" > 
							<%=vList[i].dc_category_name%>
						</td>
					</tr>
				<%	
					} 
				%>
				</table>
 
			</td>
			
		</tr>
	</table>
	
	
	
</form>


